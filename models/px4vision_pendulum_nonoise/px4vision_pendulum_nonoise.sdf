<?xml version='1.0'?>
<sdf version='1.6'>
  <enable_lockstep>false</enable_lockstep>
  <model name='px4vision_pendulum_nonoise'>
  <static>false</static>
  <include>
    <uri>model://px4vision_nonoise</uri>
  </include>

  <link name='pendulum'>
    <pose>-0.5 0 0 0 1.57 0</pose>
    <inertial>
      <mass>0.01</mass>
      <inertia>
        <ixx>0.0008333</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>0.0008333</iyy>
        <iyz>0</iyz>
        <izz>0.00005</izz>
      </inertia>
    </inertial>
    <collision name='pendulun_collision'>
      <geometry>
        <cylinder>
          <radius>0.01</radius>
          <length>1.0</length>
        </cylinder>
      </geometry>
      <surface>
        <contact>
          <ode/>
        </contact>
        <friction>
          <ode/>
        </friction>
      </surface>
    </collision>
    <visual name='pendulum_visual'>
      <geometry>
        <cylinder>
          <radius>0.01</radius>
          <length>1.0</length>
        </cylinder>
      </geometry>
      <material>
        <script>
          <name>Gazebo/White</name>
          <uri>file://media/materials/scripts/gazebo.material</uri>
        </script>
      </material>
    </visual>
    <gravity>1</gravity>

  </link>

  <link name='load'>
    <pose>-1 0 0 0 1.57 0</pose>
    <inertial>
      <mass>0.2</mass>
      <inertia>
        <ixx>0.0008</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>0.0008</iyy>
        <iyz>0</iyz>
        <izz>0.0008</izz>
      </inertia>
    </inertial>

    <collision name='load_collision'>
      <geometry>
        <sphere>
          <radius>0.1 0.1 0.1</radius>
        </sphere>
      </geometry>
      <surface>
        <contact>
          <ode/>
        </contact>
        <friction>
          <ode/>
        </friction>
      </surface>
    </collision>

    <visual name='load_visual'>
      <geometry>
        <sphere>
          <radius>0.1 0.1 0.1</radius>
        </sphere>
      </geometry>
      <material>
        <script>
          <name>Gazebo/Red</name>
          <uri>file://media/materials/scripts/gazebo.material</uri>
        </script>
      </material>
    </visual>
    <gravity>1</gravity>
  </link>

  <joint name='load_joint' type='fixed'>
    <pose>0 0 0.0 0 0 0</pose>
    <child>load</child>
    <parent>pendulum</parent>
  </joint>

  <joint name='pendulum_joint' type='universal'>
    <pose>0 0 0.5 0 0 0</pose>
    <!-- <axis>
      <xyz>1 0 0</xyz>
    </axis>
    <axis2>
      <xyz>0 1 0</xyz>
    </axis2> -->
    <child>pendulum</child>
    <parent>px4vision_nonoise::base_link</parent>
  </joint>


  </model>
</sdf>
